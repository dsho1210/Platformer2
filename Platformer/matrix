matrix = [  [3,6,1,8,1,3,9],
            [4,7,1,2,1,2,4],
            [1,1,4,3,6,2,1],
            [3,5,1,3,2,2,0],
            [5,5,5,3,2,1,1]   ]

for j,  row in enumerate(matrix):
    for i, number in enumerate(row):
        if number == 1:
            print(i, j)



for i in range(len(matrix)):
    for j in range(len(matrix[0])):
        if matrix[i][j]==1:
            print(i, j)